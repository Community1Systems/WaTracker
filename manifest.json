{
    "manifest_version": 2,
    "name": "WaTracker",
    "description": "Tracks and stores the WhatsApp activity statuses of your friends",
    "icons": { "128": "art/icon_green_128.png" },
    "version": "0.1",

    "app": {
        "background": {
            "scripts": ["background.js"]
        }
    },
    "permissions": [
        "*://web.whatsapp.com/*",
        "webview",
        "power"
    ]
}
